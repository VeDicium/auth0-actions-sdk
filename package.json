  "description": "Auth0 Actions SDK - Containing pre-defined rules and types regarding Auth0 Actions",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run lint:build && rimraf dist",
    "build": "tsc -p tsconfig.build.json",
